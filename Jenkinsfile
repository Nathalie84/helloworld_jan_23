pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello'
                sleep 10
            }
        }
        stage('Build') {
            steps {
                echo 'Build'
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
                sleep 10
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy'
            }
        }
    }
}



